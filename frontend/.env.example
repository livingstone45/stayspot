# API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# WebSocket Configuration
VITE_SOCKET_URL=http://localhost:5000
VITE_SOCKET_TIMEOUT=5000

# Application Configuration
VITE_APP_NAME=StaySpot
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Property Management Platform

# Environment
VITE_NODE_ENV=development

# External Services
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true

# Analytics (Optional)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
VITE_HOTJAR_ID=your_hotjar_id

# Social Media (Optional)
VITE_FACEBOOK_APP_ID=your_facebook_app_id
VITE_TWITTER_HANDLE=@stayspot

# Contact Information
VITE_SUPPORT_EMAIL=support@stayspot.com
VITE_SUPPORT_PHONE=+1-555-0123

# File Upload Limits
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_FILES_PER_UPLOAD=10
VITE_ALLOWED_FILE_TYPES=jpg,jpeg,png,pdf,doc,docx

# Map Configuration
VITE_DEFAULT_MAP_CENTER_LAT=40.7128
VITE_DEFAULT_MAP_CENTER_LNG=-74.0060
VITE_DEFAULT_MAP_ZOOM=12

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_OFFLINE_CACHE_DURATION=86400000